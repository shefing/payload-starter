{
  "name": "cmweb2",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "bump-payload": "payload run ./src/scripts/bump-payload.ts ./package.json && pnpm i",
    "importUsers": "payload run ./src/scripts/import/importUsers.ts ./package.json && pnpm i",
    "importUniversities": "payload run ./src/scripts/import/importUniversities.ts ./package.json && pnpm i",
    "regenerateMediaSizes": "payload run ./src/scripts/regenerateMediaSizes.ts ./package.json && pnpm i",
    "refRegenerate": "payload run ./src/scripts/refRegenerate.ts ./package.json && pnpm i",
    "runScript": "payload run ./src/scripts/standalone-script.ts ./package.json && pnpm i",
    "dev": "pnpm generate:types && pnpm generate:importmap && cross-env NODE_OPTIONS=\"--inspect --no-deprecation\" next dev",
    "devturbo": "pnpm generate:types && cross-env NODE_OPTIONS=\"--inspect --no-deprecation\" next dev --turbo",
    "dev:seed": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts PAYLOAD_DROP_DATABASE=true PAYLOAD_SEED=true nodemon",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:types": "payload generate:types && copyfiles -V -f ../website/src/payload-types.ts ./src/",
    "generate:importmap": "payload generate:importmap",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS='--no-deprecation --enable-source-maps=true' next start",
    "reinstall": "rm -rf node_modules .next && rm -rf .next && rm pnpm-lock.yaml  && pnpm --ignore-workspace install"
  },
